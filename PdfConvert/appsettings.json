{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Warning",
      "Microsoft.AspNetCore.Mvc.Infrastructure.FileStreamResultExecutor":"Warning",
      "Microsoft.AspNetCore.Mvc.Infrastructure.ControllerActionInvoker":"Warning"
    }
  },
  "MaxConcurrentRequestsOptions": {
    "MaxQueueLength": 200,
    "MaxTimeInQueue": 60000,
    "Limit": 5,
    "LimitExceededPolicy": 1
  },
  "AllowedHosts": "*"
}
